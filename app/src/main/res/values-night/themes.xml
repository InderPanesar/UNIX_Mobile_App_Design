        <item name="colorPrimary">@color/cinder</item>
        <item name="colorPrimaryVariant">@color/cinder</item>
        <item name="colorOnPrimary">@color/titan_white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/tuna</item>
        <item name="colorSecondaryVariant">@color/tuna</item>
        <item name="colorOnSecondary">@color/titan_white</item>
